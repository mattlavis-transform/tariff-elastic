{
    "settings": {
        "index": {
            "analysis": {
                "analyzer": {
                    "complex_synonyms": {
                        "tokenizer": "keyword",
                        "filter": [
                            "stem_exclusions",
                            "asciifolding",
                            "lowercase",
                            "english_stemmer",
                            "synonyms_drugs",
                            "synonyms_generic",
                            "synonyms_trees",
                            "synonyms_food",
                            "synonyms_flowers",
                            "synonyms_cheeses",
                            "synonyms_animals",
                            "synonyms_us_english",
                            "my_en_US_dict_stemmer"
                        ]
                    },
                    "english_exact": {
                        "tokenizer": "standard",
                        "filter": [
                            "lowercase"
                        ]
                    },
                    "english": {
                        "tokenizer": "standard",
                        "filter": [
                            "stem_exclusions",
                            "english_possessive_stemmer",
                            "lowercase",
                            "english_stop",
                            "english_stemmer"
                        ]
                    },
                    "my_hunspell": {
                        "tokenizer": "standard",
                        "filter": [
                            "my_en_US_dict_stemmer"
                        ]
                    }
                },
                "filter": {
                    "stem_exclusions": {
                        "type": "keyword_marker",
                        "keywords_path": "stem_exclusions.txt"
                    },
                    "english_stop": {
                        "type": "stop",
                        "stopwords": "_english_"
                    },
                    "english_stemmer": {
                        "type": "stemmer",
                        "language": "english"
                    },
                    "english_possessive_stemmer": {
                        "type": "stemmer",
                        "language": "possessive_english"
                    },
                    "my_en_US_dict_stemmer": {
                        "type": "hunspell",
                        "locale": "en_US",
                        "dedup": false
                    },
                    "synonyms_drugs": {
                        "type": "synonym",
                        "synonyms_path": "synonyms_drugs.txt",
                        "ignore_case": true,
                        "updateable": true
                    },
                    "synonyms_generic": {
                        "type": "synonym",
